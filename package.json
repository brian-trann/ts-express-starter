{
  "name": "ts-express-starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build-swc": "swc src -d dist -s",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "generate-route": "npx ts-node scripts/generate-route.ts",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "start": "npm run build-ts && npm run serve",
    "test": "jest --forceExit --coverage --verbose",
    "print-secrets": "npx ts-node scripts/print-secrets.ts",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "validate-secrets": "npx ts-node scripts/validate-secrets.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^7.16.0",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "dotenv-parse-variables": "^2.0.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.1",
    "kafkajs": "^1.15.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@elastic/elasticsearch-mock": "^0.3.1",
    "@swc/cli": "^0.1.53",
    "@swc/core": "^1.2.119",
    "@types/dotenv-parse-variables": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.12",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": "^16.13.0"
  }
}
